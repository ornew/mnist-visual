.ornew
  section.contents
    min-width 20em
    max-width 40em
    margin 0 auto
    padding 1em

  canvas
    display block
    width 280px
    height 280px
    padding 0
    margin 1em auto

  .buttons
    display flex
    justify-content center
    .button
      margin 0 1em

  .contents
    .center
      margin 1em 0
      text-align center

  .progress-circular
    transition opacity .3s

#canvas
  border dashed 1px gray

#log
  time
    color gray
    margin 0 1em

#inference
  font-size 3em

#step_display
  display inline-block
  width 5em

#test-results
  table
    margin 0 auto
  th
    width 30px
    height 30px
  td
    overflow hidden
    width 30px
    height 30px
    font-size 6px
    border 1px solid
    border-radius 30px
    text-align center

